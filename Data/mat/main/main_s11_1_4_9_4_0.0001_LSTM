#!/bin/sh
/home/ctr/MATLAB/R2021a/bin/matlab -nodesktop -nosplash <<EOF
clear;
addpath(genpath('/home/ctr/git/FalBench/breach'));
addpath(genpath('/home/ctr/git/FalBench'));
s11 = STL_Formula('phi','alw_[0,24](b[t] < 80)');
addpath(['STL_TO_SMT'],['STL_TO_SMT' filesep 'infix2prefix']);

switchpt = 9;
T = 24.0;
input_num = 1;
input_range = [0.0 1.0];
output_range = [0.0 100.0];
controlpt = 4;
num_var = 4;
inputf = 's11_1_4_9_4_0.0001_LSTM/input_traces';
outputf = 's11_1_4_9_4_0.0001_LSTM/output_traces';
step_size = 1.0;
delta_range = [0 20.0];
h = 0.0001;

cd 'STL_TO_SMT/'
set(0,'RecursionLimit',1300);
gt = TraceGenerator(inputf, outputf, num_var, controlpt, T, switchpt, step_size, input_num, input_range, output_range, h, s11, delta_range);
gt.gen();
fal_tr = gt.fal_tr;
save('/home/ctr/git/FalBench/STL_TO_SMT/Faltr/s11_1_4_9_4_0.0001_LSTM_faltr.mat', 'fal_tr');
